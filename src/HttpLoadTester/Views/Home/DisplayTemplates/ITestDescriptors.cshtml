@model HttpLoadTester.Services.ITestDescriptors

<div class="panel-heading">Simulating Users Viewing PFM Dashboard </div>
<div id ="@string.Format("testContainer{0}",Model.Name)" class="panel-body">


    <div class="container">
        <div class="row">
            <div class="btn-group btn-group-lg" role="group" aria-label="Simulates PFM user views">
                <button id="usrBtnStartView" type="button" data-testname="@Model.Name" class="btn btn-default sendStartToHub">Start</button>
                <button id="usrBtnStopView" type="button" data-testname="@Model.Name" class="btn btn-default sendStopToHub">Stop</button>
            </div>
        </div>
        <div class="row">
            <div class="col-md-5">
                <div class="table-responsive">
                    <table id="statusTable" class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th>Status</th>
                                <th>Count</th>
                                <th>AverageDuration</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-6">
                <canvas id="chartContainer"></canvas>

            </div>
        </div>
    </div>
</div>

