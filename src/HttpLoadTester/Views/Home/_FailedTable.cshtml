<div id="failedRequests">
    <h2>Last 50 Failed Requests</h2>
    <div class="table-responsive">
        <table id="statusTable" class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>Request Type</th>
                    <th>Time</th>
                    <th>Response Code</th>
                    <th>Duration (ms)</th>
                    <th>Error Message</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>


<script id="failedRequestsTemplate" type="text/x-handlebars-template">
        {{#exceptions}}
            <tr>
                <td>{{testName}}</td>
                <td>{{time}}</td>
                <td>{{responseCode}}</td>
                <td>{{duration}}</td>
                <td>{{message}}</td>
            </tr>
        {{/exceptions}}
</script>
